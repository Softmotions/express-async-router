{
	"compilerOptions": {
		"target": "es2015",
    "moduleResolution": "node",
		"module": "commonjs",
		"lib": [
      "es2017",
      "dom"
		],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
    "noImplicitThis": false
	},
	"exclude": [
    "node_modules/**",
    "**/*.aot.ts"
  ]
}